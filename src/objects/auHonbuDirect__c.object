    <webLinks>
        <fullName>Archive</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>アーカイブ/アーカイブ削除</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)} 

var newRecords = []; 

if({!auHonbuDirect__c.IsArchived__c}){
var retVal = confirm(&quot;アーカイブ削除するか。&quot;); 
if (retVal == true){
var objectToUpdate = new sforce.SObject(&quot;auHonbuDirect__c&quot;); 
objectToUpdate.id =&quot;{!auHonbuDirect__c.Id}&quot;; 
objectToUpdate.IsArchived__c=false;
newRecords.push(objectToUpdate);
sforce.connection.update(newRecords);
}
}else{
var retVal = confirm(&quot;アーカイブするか。&quot;); 
if (retVal == true){
var objectToUpdate = new sforce.SObject(&quot;auHonbuDirect__c&quot;); 
objectToUpdate.id =&quot;{!auHonbuDirect__c.Id}&quot;; 
objectToUpdate.IsArchived__c=true;
newRecords.push(objectToUpdate);
sforce.connection.update(newRecords);
}
}

window.location.reload();</url>
    </webLinks>
    <webLinks>
        <fullName>Delete_All_KDDI_User</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Delete All KDDI User</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)} 

var retVal = confirm(&quot;KDDIユーザーを削除してよろしいですか。&quot;); 
if (retVal == true) 
{ 

var recordsToDeleteRequest=&quot;Select Id From auHonbuDirectDetail__c Where auHonbuDirect__r.Id=&apos;{!auHonbuDirect__c.Id}&apos; And Type__c=&apos;KDDI_ACCESS&apos; Limit 2&quot;; 
var recordsToDelete=sforce.connection.query(recordsToDeleteRequest); 
var recordsIdToDelete; 
var idsForDeletion; 


while(recordsToDelete.getArray(&quot;records&quot;).length!=0){ 

recordsToDelete=sforce.connection.query(recordsToDeleteRequest); 
recordsIdToDelete = recordsToDelete.getArray(&quot;records&quot;); 

idsForDeletion = []; 
for (var i=0; i&lt;recordsIdToDelete.length; i++) { 
idsForDeletion.push(recordsIdToDelete[i].Id); 
} 

sforce.connection.deleteIds(idsForDeletion); 

} 

var sharingToDeleteRequest=&quot;Select Id From auHonbuDirect__share Where ParentId = &apos;{!auHonbuDirect__c.Id}&apos; And RowCause = &apos;shared_To_This_KDDI_User__c&apos; Limit 2&quot;; 
var sharingToDelete=sforce.connection.query(sharingToDeleteRequest); 
var sharingIdToDelete; 
var idsForSharingDeletion; 

while(sharingToDelete.getArray(&quot;records&quot;).length!=0){ 

sharingToDelete=sforce.connection.query(sharingToDeleteRequest); 
sharingIdToDelete = sharingToDelete.getArray(&quot;records&quot;); 

idsForSharingDeletion = []; 
for (var i=0; i&lt;sharingIdToDelete.length; i++) { 
idsForSharingDeletion.push(sharingIdToDelete[i].Id); 
} 
sforce.connection.deleteIds(idsForSharingDeletion); 

} 

alert(&apos;KDDIユーザーが削除されました。&apos;); 
}else{ 

} 
window.location.reload();</url>
    </webLinks>
