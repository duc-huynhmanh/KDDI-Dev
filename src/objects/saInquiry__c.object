    <fields>
        <fullName>IsValid__c</fullName>
        <externalId>false</externalId>
        <formula>OR(
AND(
TEXT(Status__c) == &apos;Saved&apos;,
TEXT(Type__c) == &apos;DAILY_REPORT&apos;,
OR(ISNULL(ValidFrom__c), ValidFrom__c &lt;= TODAY()),
OR(ISNULL(ValidTo__c), ValidTo__c &gt;= TODAY())
),
AND(
TEXT(Status__c) == &apos;Saved&apos;,
TEXT(Type__c) != &apos;DAILY_REPORT&apos;,
OR(ISNULL(ValidFromDatetime__c), ValidFromDatetime__c &lt;= NOW()),
OR(ISNULL(ValidToDatetime__c), ValidToDatetime__c &gt;= NOW())
)
)</formula>
