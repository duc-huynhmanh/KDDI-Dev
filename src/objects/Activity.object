        <fullName>CSVExportPart1__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISNULL(AuNewMNP__c), &quot;&quot;, TEXT(AuNewMNP__c)) + &quot;,&quot; +
IF(ISNULL(AuNewTablet__c), &quot;&quot;, TEXT(AuNewTablet__c)) + &quot;,&quot; +
IF(ISNULL(AuNewRooter__c), &quot;&quot;, TEXT(AuNewRooter__c)) + &quot;,&quot; +
IF(ISNULL(AuModelChange__c), &quot;&quot;, TEXT(AuModelChange__c)) + &quot;,&quot; +
IF(ISNULL(AuTotal__c), &quot;&quot;, TEXT(AuTotal__c)) + &quot;,&quot; +
IF(ISNULL(BBCHikari__c), &quot;&quot;, TEXT(BBCHikari__c)) + &quot;,&quot; +
IF(ISNULL(BBCCableTosu__c), &quot;&quot;, TEXT(BBCCableTosu__c)) + &quot;,&quot; +
IF(ISNULL(UQ_2__c), &quot;&quot;, TEXT(UQ_2__c)) + &quot;,&quot; +
IF(ISNULL(UQ_3__c), &quot;&quot;, TEXT(UQ_3__c)) + &quot;,&quot; +
IF(ISNULL(auDenki__c), &quot;&quot;, TEXT(auDenki__c)) + &quot;,&quot; +
IF(ISNULL(WALLET_Market__c), &quot;&quot;, TEXT(WALLET_Market__c)) + &quot;,&quot; +
IF(ISNULL(WaterServer__c), &quot;&quot;, TEXT(WaterServer__c)) + &quot;,&quot;</formula>
        <label>CSVExportPart1</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CSVExportPart2__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(OwnerDepartment__c), &quot;&quot;, &quot;\&quot;&quot; + SUBSTITUTE(OwnerDepartment__c, &quot;\&quot;&quot;, &quot;\&quot;\&quot;&quot;) + &quot;\&quot;&quot;) + &quot;,&quot; +
&quot;\&quot;&quot; + SUBSTITUTE(Owner:User.LastName, &quot;\&quot;&quot;, &quot;\&quot;\&quot;&quot;) + IF(ISNULL(Owner:User.FirstName), &quot;&quot;, &quot; &quot; + Owner:User.FirstName) + &quot;\&quot;,&quot; +
&quot;\&quot;&quot; + IF(ISNULL(Owner:User.KDDIEmployeeId__c), &quot;&quot;, &quot; &quot; + Owner:User.KDDIEmployeeId__c) + &quot;\&quot;,&quot; +
&quot;\&quot;&quot; + SUBSTITUTE(Owner:User.UserRole.Name, &quot;\&quot;&quot;, &quot;\&quot;\&quot;&quot;) + &quot;\&quot;,&quot; +
IF(TEXT(Type__c) == &quot;その他&quot;, &quot;\&quot;その他\&quot;&quot;, IF(ISBLANK(TEXT(Category__c)), &quot;&quot;, &quot;\&quot;&quot; + TEXT(Category__c) + &quot;\&quot;&quot;)) + &quot;,&quot; +
&quot;\&quot;&quot; + IF(IsPrivate, &quot;(非公開) &quot;, &quot;&quot;) + SUBSTITUTE(Subject, &quot;\&quot;&quot;, &quot;\&quot;\&quot;&quot;) + &quot;\&quot;,&quot; +
&quot;\&quot;&quot; + IF(ActivityReportSent__c, &quot;1&quot;, &quot;0&quot;) + &quot;\&quot;,&quot;</formula>
        <label>CSVExportPart2</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CSVExportPart3__c</fullName>
